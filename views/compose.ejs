<%- include("partials/header") -%>
  <h1>Compose</h1>
  <form class="" action="/compose" method="post">
    <div class="form-group">
      <label>Title</label>
      <input type="text" class="form-control" name="postTitle"  autocomplete="off" >
    </div>
    <div class="form-group">
      <label>Post</label>
      <textarea name="postBody"  class="form-control" rows="8" cols="50"></textarea>
    </div>
    <button class="btn btn-primary" type="submit"> Publish</button>
  </form>

<%- include("partials/footer") -%>
